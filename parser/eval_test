even = 0
odd = 0
sum = 0
i = 1
while i < 10 {
    if i % 2 == 0 {     // event number ?
        even = even + i
    } else {
        odd = odd + i
    }
    sum = sum + (i + 1)
    i = i + 1
}
even
-odd
"sum = " + sum
"Hello " + "World " + sum

def fib(n) {
    if n < 2 {
        n
    } else {
        fib(n - 1) + fib(n - 2)
    }
}
"fib(10) == " + fib(10)
