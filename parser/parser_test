even = 0
odd = 0
i = 1
while i < 10 {
    if i % 2 == 0 {     // event number ?
        even = even + i
    } else {
        odd = odd + i
    }
    i = i + 1
}
event + -odd

def fib(n) {
    if n < 2 {
        n
    } else {
        fib(n - 1) + fib(n - 2)
    }
}
fib(10)

def counter(c) {
    func () {c = c + 1}
}
c1 = counter(1)
c1()
c1()
